              <mj-text font-size="14px" font-family="Muli, Arial, sans-serif" color="#ffffff" align="center"><b><a href="tel:{{rds.B2B_RDS.account_manager_info(contact.8061)[0].phone_number}}, {{rds.B2B_RDS.account_manager_info(contact.8061)[0].phone_number_extension}}" style="color:#27bdbe;text-decoration:none;">{{rds.B2B_RDS.account_manager_info(contact.8061)[0].phone_number}} {{rds.B2B_RDS.account_manager_info(contact.8061)[0].phone_number_extension}}</a> | <a href="mailto:{{rds.B2B_RDS.account_manager_info(contact.8061)[0].email}}" style="text-decoration:none; color:#27bdbe; font-family:'Muli', Arial, sans-serif;">{{rds.B2B_RDS.account_manager_info(contact.8061)[0].email}}</a></b></mj-text>
              <mj-spacer height="25px" />

            </mj-column>
          </mj-section>

          <mj-raw>{% endif %}</mj-raw>


    <mj-raw>
      {% endif %}
    </mj-raw>


    <!-- end performance report -->

    <mj-raw><div class="div-wrap-wide"></mj-raw>
    <mj-section>
      <mj-column>
        <mj-spacer height="25px" />
        <mj-image width="340px" src="http://suite29.emarsys.net/custloads/789451948/covetrus-footer-logo.png" href="http://vetsfirstchoice.com/" />
        <mj-spacer height="20px" />
      </mj-column>
    </mj-section>


    <mj-section padding="10px 0 25px 0 ">
      <mj-column>
        <mj-raw>
          <table width="100%">
            <tr>
              <td padding="20px" style="text-align:center;border-right:1px solid #c9c5c5;"><a href="http://roadrunnerpharmacy.com/"><img src="http://suite29.emarsys.net/custloads/789451948/Welcome_Series/Send3/Roadrunner.png" alt="Roadrunner Pharmacy" width="100%" style="max-width:150px;"></a></td>
              <td padding="20px" style="text-align:center;"><a href="http://www.atlasdrugs.com/"><img src="http://suite29.emarsys.net/custloads/789451948/Welcome_Series/Send3/Atlas.png" alt="Atlas Pharmaceuticals" width="100%" style="max-width:150px;"></a></td>
