              <mj-text container-background-color="#ffffff" padding="10px 10px 0" color="#021660" font-size="40px" font-weight="700" line-height="48px" font-family="Open Sans, Arial, sans-serif" align="center">
                ${{rds.B2B_RDS.practice_engagement_current(contact.8077)[0].practice_renewables_value|round(0,'floor')}}
              </mj-text>
              <mj-text container-background-color="#ffffff" padding="0 10px 20px" color="#021660" font-size="16px" line-height="18px" font-weight="700" font-family="Open Sans, Arial, sans-serif" align="center" letter-spacing="1px">TOTAL REVENUE OPPORTUNITY</span></mj-text>
            </mj-column>

          </mj-section>
          <mj-raw></div></mj-raw>

          <!-- Account Manager section -->

          <mj-raw>{% if rds.B2B_RDS.account_manager_info(contact.8061)[0].email is empty %}</mj-raw>

