<mjml>
  <mj-head>
    <mj-title></mj-title>
    <mj-font name="Mulish" href="https://fonts.googleapis.com/css?family=Mulish:400,600,700,800" rel="stylesheet"/>
    <mj-attributes>
      <mj-breakpoint width="480px" />
      <mj-all padding="0" font-family="'Mulish', Arial, sans-serif" background-color="#ffffff"/>

      <mj-class name="h1" font-size="38px" line-height="44px" font-family="Georgia, serif" align="left" color="#021660" font-weight="700"/>
      <mj-class name="h2" font-size="32px" line-height="36px" font-family="Georgia, serif" align="left" color="#021660" font-weight="700"/>
      <mj-class name="h3" font-size="28px" line-height="34px" font-family="'Mulish', Arial, sans-serif" align="left" color="#021660" font-weight="700"/>
      <mj-class name="h4" font-size="24px" line-height="32px" font-family="'Mulish', Arial, sans-serif" align="left" color="#021660" font-weight="700"/>
      <mj-class name="h5" font-size="22px" line-height="28px" font-family="'Mulish', Arial, sans-serif" align="left" color="#021660" font-weight="700"/>
      <mj-class name="h6" font-size="15px" line-height="23px" font-family="'Mulish', Arial, sans-serif" align="left" color="#021660" font-weight="800" text-transform="uppercase" letter-spacing="1.5px"/>
      <mj-class name="lead" font-size="20px" line-height="28px" font-family="'Mulish', Arial, sans-serif" font-weight="400" align="left" color="#2c2c2c"/>

      <mj-text font-size="16px" line-height="24px" align="left" color="#2c2c2c"/>

      <mj-class name="mousetype" font-size="12px" line-height="16px" align="left" color="#8d8d8d" />

      <!--
      **** BRAND COLORS ****

        - Base Colors -
            Navy: #021660
            Deep teal: #01818c

        - Accent Colors -
            Bright teal: #27bdbe
            Red: #ed3030

        - Additional Background Colors -
            Gray: #f7f7f7
            Light Blue: #e8f7ff

      -->

    </mj-attributes>
    <mj-style inline="inline">
      a {
        font-family: 'Mulish', Arial, sans-serif;
        font-weight: inherit;
        color: #2c2c2c;
      }

      a[x-apple-data-detectors] {
        color: inherit !important;
        text-decoration: none !important;
        font-size: inherit !important;
        font-family: inherit !important;
        font-weight: inherit !importantSho
        line-height: inherit !important;
      }

      .wrap > table > tbody > tr > td {
        padding: 10px 30px !important;
      }

      .padding-right > table > tbody > tr > td {
        padding-right: 30px !important;
      }

      .wrap-narrow > table > tbody > tr > td {
        padding: 10px 65px !important;
      }

      .wrap-wide > table > tbody > tr > td {
        padding: 10px 15px !important;
      }

      .wrap-sides > table > tbody > tr > td {
        padding: 0px 30px !important;
      }
      .wrap-sides-narrow > table > tbody > tr > td {
        padding: 0px 65px !important;
      }

      .wrap-hero {
     	  padding: 30px !important;
      }

      .wrap-hero-button {
       	padding: 30px !important;
      }

      .div-wrap > div > table > tbody > tr > td {
        padding: 10px 65px !important;
      }

      .div-wrap-wide > div > table > tbody > tr > td {
        padding: 10px 30px !important;
      }
      .bg-padding{
        padding: 40px 60px;
      }
      p{
        margin:0;
      }
      .col-1 .row-blue{
        background-color:#d8edf8;
      }
      .col-1 .row-white{
        background-color:#ffffff;
      }
      .col-2 .row-blue{
        background-color:#d8edf8;
      }
      .col-2 .row-white{
        background-color:#ffffff;
      }

    </mj-style>
    <mj-raw>
      <!--[if gte mso 9]>
      <style>
        div.list {Margin: 0 0 0 -20px;}
      </style>
      <![endif]-->
    </mj-raw>
    <mj-style>

      .button-teal:hover {
        background-color: #27bdbe;
      }

      .button-blue:hover {
        background-color: #003a9f;
      }
      .button-red:hover {
        background-color: #ef4e4e;
      }

      .mobile-hide {
        display: block;
      }
      .mobile-show {
        display: none;
      }
      .one-line {
        display: block;
        width:100%;
      }

      .munge table,
      .munge table{
        width:100%!important;
      }

      /* Alt text styles */
      .alt-text-logo img {
        font-size: 20px!important;
        font-weight: 700;
        font-family:'Mulish', Arial, Sans-serif;
        color: #021660;
      }
      div.list ul {
        padding-inline-start: 20px ;
        -webkit-padding-start: 20px;
        margin-top: 0;
      }

      div.list li {
        list-style: disc outside;
        mso-special-format: bullet;
        margin-bottom: 3px;
        mso-margin-bottom-alt: 3px;
      }


      @media all and (max-width: 480px){
        .wrap  > table > tbody > tr > td,
        .wrap-wide  > table > tbody > tr > td,
        .wrap-narrow  > table > tbody > tr > td {
          padding: 10px 15px !important;
        }
        .bg-padding{
          padding: 40px 15px !important;
        }

        .wrap-sides  > table > tbody > tr > td,
        .wrap-sides-narrow  > table > tbody > tr > td {
          padding: 0px 15px !important;
        }

        .padding-right  > table > tbody > tr > td {
          padding-right: 0px!important;
        }

        .one-line {
          display: inline !important;
          width: auto !important;
        }

        .subhead {
          padding: 0 10px 25px 10px !important;
        }

        .div-wrap > div > table > tbody > tr > td {
          padding: 10px !important;
        }

        .div-wrap-wide > div > table > tbody > tr > td {
          padding: 10px !important;
        }

        .mobile-centered * {
          text-align: center !important;
          margin:0 auto;
        }

        .mobile-hide {
          display: none !important;
        }

        .mobile-show {
          display: block !important;
        }

        .wrap-hero,
        .wrap-hero-button {
          padding: 10px 15px !important;
        }

        .button-size {
          max-width: 600px !important;
          width: 100% !important;
        }

        .button-size a {
          display: block !important;
        }
        .col-2 .row-blue{
          background-color:#ffffff !important;
        }
        .col-2 .row-white{
          background-color:#d8edf8 !important;
        }
      }

    </mj-style>


  </mj-head>
  <mj-body>



